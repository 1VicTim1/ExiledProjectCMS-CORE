{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8080"
      },
      "Https": {
        "Url": "https://0.0.0.0:8443",
        "Certificate": {
          "Path": "/app/ssl/cms-api.crt",
          "KeyPath": "/app/ssl/cms-api.key"
        }
      },
      "HttpsInternal": {
        "Url": "https://0.0.0.0:8444",
        "Certificate": {
          "Path": "/app/ssl/cms-api.crt",
          "KeyPath": "/app/ssl/cms-api.key"
        },
        "ClientCertificateMode": "RequireCertificate"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/app/ssl/cms-api.crt",
        "KeyPath": "/app/ssl/cms-api.key"
      }
    }
  },
  "HttpsRedirection": {
    "RedirectStatusCode": 308,
    "HttpsPort": 8443
  },
  "SSL": {
    "RequireHttps": false,
    "RequireHttpsForInternal": true,
    "ClientCertificateValidation": true,
    "TrustedCA": "/app/ssl/ca.crt",
    "InterServiceAuth": {
      "Enabled": true,
      "RequireClientCertificate": true,
      "AllowedCertificates": [
        "/app/ssl/clients/internal-client-client.crt"
      ]
    }
  },
  "ConnectionStrings": {
    "Redis": "rediss://redis:6380,ssl=true,sslHost=redis.exiled.local,password=${REDIS_PASSWORD}",
    "DefaultConnection": "${DB_CONNECTION_STRING_SSL}"
  },
  "ExternalServices": {
    "GoAPI": {
      "BaseUrl": "https://go-api:8443",
      "ClientCertificatePath": "/app/ssl/clients/internal-client-client.crt",
      "ClientCertificateKeyPath": "/app/ssl/clients/internal-client-client.key",
      "TrustedCA": "/app/ssl/ca.crt",
      "Timeout": 30000
    },
    "SkinsService": {
      "BaseUrl": "https://skins-service:8443",
      "ClientCertificatePath": "/app/ssl/clients/internal-client-client.crt",
      "ClientCertificateKeyPath": "/app/ssl/clients/internal-client-client.key",
      "TrustedCA": "/app/ssl/ca.crt"
    },
    "EmailService": {
      "BaseUrl": "https://email-service:8443",
      "ClientCertificatePath": "/app/ssl/clients/internal-client-client.crt",
      "ClientCertificateKeyPath": "/app/ssl/clients/internal-client-client.key",
      "TrustedCA": "/app/ssl/ca.crt"
    }
  },
  "Logging": {
    "LogLevel": {
      "Microsoft.AspNetCore.HttpsPolicy": "Information",
      "Microsoft.AspNetCore.Authentication.Certificate": "Information"
    }
  }
}