{
  "DatabaseProvider": "SqlServer",
  "CacheProvider": "Memory",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ExiledProjectCMS;Trusted_Connection=true;MultipleActiveResultSets=true;",
    "MySQL": "Server=localhost;Database=ExiledProjectCMS;Uid=root;Pwd=password;CharSet=utf8mb4;",
    "PostgreSQL": "Host=localhost;Database=ExiledProjectCMS;Username=postgres;Password=password;",
    "Redis": "localhost:6379"
  },
  "Cache": {
    "DefaultExpiration": "00:30:00",
    "News": {
      "Expiration": "00:15:00"
    },
    "Users": {
      "Expiration": "01:00:00"
    }
  },
  "Admin": {
    "DefaultUsername": "admin",
    "RequireInitialSetup": true
  },
  "Plugins": {
    "Directory": "Plugins",
    "EnableHotReload": true,
    "AllowedExtensions": [
      ".dll",
      ".js",
      ".go"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
    }
  },
  "AllowedHosts": "*"
}
